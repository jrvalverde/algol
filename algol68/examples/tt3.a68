PROGRAM tt3 CONTEXT VOID
USE standard
BEGIN
LONG INT max = long max int % LONG 100;

LONG INT li:=LONG 1;
INT mod:=1;

WHILE li < max
DO
   print((whole(li,long int width+1),blank,
          fixed(li,long int width+1,0),blank,
			 float(li,long int width+5,long int width-1,3),
			 newline));
   li*:=LONG 10+:=LENG((mod+:=1)MOD 10)
OD

END
FINISH